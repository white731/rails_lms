

<%= form_with(model:[@course, @enrollment], local:true, class: 'ui form') do |f|%>
<%= f.label :user%>
<%= f.select :user_id, @users.collect{|u| [u.full_name, u.id]}%>
<%= f.label :role%>
<%= f.select :role, [["TA", "TA"],["Student", "Student"],["Teacher","Teacher"]]%>
<br>
<%= f.submit nil, class: 'ui green button'%>
<%end%>